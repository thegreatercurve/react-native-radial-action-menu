{
  "name": "react-native-radial-context-menu",
  "version": "0.1.0",
  "description": "A react-native implementation of a radial action menu",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./build",
    "format": "prettier --trailing-comma --write \"./src/**/*.ts?(x)\"",
    "lint": "tslint -c tslint.json \"./src/**/*.ts?(x)\"",
    "prepare": "npm run clean && npm run build",
    "start": "tsc -w",
    "test": "jest"
  },
  "main": "build/index.js",
  "typings": "index.d.ts",
  "keywords": [
    "radial",
    "action",
    "menu",
    "radial",
    "menu"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thegreatercurve/react-native-radial-context-menu.git"
  },
  "author": "John Flockton",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/react": "^16.9.35",
    "@types/react-native": "^0.57.65",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.0.1",
    "babel-preset-react-native": "^4.0.1",
    "jest": "^26.0.1",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-native": "^0.62.2",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.9.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "dependencies": {}
}
